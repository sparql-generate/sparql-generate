@prefix ex:    <http://example.com/> .

// regex for UUID version 4, since UUIDs are random can not test for a specific value
ex:{/^[0-9A-F]{8}-[0-9A-F]{4}-[4][0-9A-F]{3}-[89AB][0-9A-F]{3}-[0-9A-F]{12}$/i}
        a        ex:Student ;
        ex:dob   "13/02/1987" ;
        ex:name  "Sam" .

ex:{/^[0-9A-F]{8}-[0-9A-F]{4}-[4][0-9A-F]{3}-[89AB][0-9A-F]{3}-[0-9A-F]{12}$/i}
        a        ex:Student ;
        ex:dob   "10/06/1990" ;
        ex:name  "Tim" .

ex:{/^[0-9A-F]{8}-[0-9A-F]{4}-[4][0-9A-F]{3}-[89AB][0-9A-F]{3}-[0-9A-F]{12}$/i}
        a        ex:Student ;
        ex:dob   "12/05/1980" ;
        ex:name  "John" .

